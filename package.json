{
  "name": "phaser-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:watch": "tsc -w --sourceMap -p .",
    "dev:serve": "http-server",
    "dev:open": "opener http://localhost:8080",
    "start": "npm-run-all --parallel dev:*",
    "build:ts": "tsc -p .",
    "build:minify": "uglifyjs dist/phaser.js dist/game.js -o dist/game.min.js",
    "deploy": "node scripts/deploy.js"
  },
  "author": "Geoffroy Warin",
  "license": "MIT",
  "devDependencies": {
    "gh-pages": "^0.9.0",
    "http-server": "^0.8.5",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^1.5.1",
    "opener": "^1.4.1",
    "rimraf": "^2.5.1",
    "typescript": "^1.8.0",
    "uglify-js": "^2.6.1"
  },
  "dependencies": {
    "phaser": "^2.4.4"
  }
}
